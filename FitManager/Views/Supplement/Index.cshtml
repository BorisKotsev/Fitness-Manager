@model List<SupplementDTO>

<a asp-action="Create">
    <button type="button" class="btn btn-success">Create</button>
</a>



<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Type</th>
            <th scope="col">Price</th>
            <th scope="col">Brand</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var supplement in Model)
        {
        <tr>
            <th scope="row">@supplement.Id</th>
            <td>@supplement.Type</td>
            <td>@supplement.Price</td>
            <td>@supplement.Brand</td>         
                <td>
     
                    <a asp-action="Delete" asp-route-id="@supplement.Id">
                        <button type="button" class="btn btn-danger">Delete</button>
                    </a>
                </td>

        </tr>
        }
    </tbody>
</table>